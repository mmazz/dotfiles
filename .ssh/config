Host github.com
    HostName github.com
    PreferredAuthentications publickey
    User git
    IdentityFile ~/.ssh/id_rsa_github

Host gitlab.com
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/id_rsa_gitlab0

Host h2.cluster.tupac.gob.ar
    User mgeiger
    HostName h2.cluster.tupac.gob.ar
    IdentitiesOnly yes
    IdentityFile ~/.ssh/id_rsa_tupac


Host gitlab.licar.exp.dc.uba.ar
    Hostname gitlab.licar.exp.dc.uba.ar
    PreferredAuthentications publickey
    User git
    IdentityFile ~/.ssh/id_rsa_licar
